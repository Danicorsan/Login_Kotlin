<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/sendFragment2">
    <fragment
        android:id="@+id/sendFragment2"
        android:name="com.example.sendmessagefragment.SendFragment"
        android:label="fragment_send"
        tools:layout="@layout/fragment_send" >
        <action
            android:id="@+id/action_sendFragment2_to_viewFragment3"
            app:destination="@id/viewFragment3" />
    </fragment>
    <fragment
        android:id="@+id/viewFragment3"
        android:name="com.example.sendmessagefragment.ViewFragment"
        android:label="fragment_view"
        tools:layout="@layout/fragment_view" >
        <argument
            android:name="message"
            app:argType="com.example.sendmessagefragment.model.Message" />
    </fragment>
</navigation>